{
  "name": "sonniydsgn-vite-starter",
  "private": true,
  "version": "1.0.0",
  "author": "sonniydsgn",
  "scripts": {
    "html": "node-w3c-validator -f lint -evH -i dist/**/*.html",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build --emptyOutDir",
