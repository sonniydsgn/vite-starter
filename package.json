{
	"name": "sonniydsgn-vite-starter",
	"private": true,
	"version": "1.1.0",
	"author": "sonniydsgn",
	"scripts": {
		"html": "node-w3c-validator -f lint -evH -i dist/**/*.html",
		"dev": "vite",
		"dev:host": "vite --host",
		"build": "vite build --emptyOutDir",
		"preview": "vite preview",
		"preview:host": "vite preview --host",
		"deploy": "vercel --prod"
	},
	"type": "module",
	"devDependencies": {
		"@studio-freight/lenis": "^1.0.33",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.2",
		"inputmask": "^5.0.8",
		"just-validate": "^4.3.0",
		"lightningcss": "^1.22.1",
		"node-w3c-validator": "^2.0.2",
		"postcss": "^8.4.32",
		"postcss-nested": "^6.0.1",
		"postcss-preset-env": "^9.3.0",
		"sharp": "^0.33.1",
		"svgo": "^3.1.0",
		"swiper": "^11.0.5",
		"vite": "^5.0.6",
		"vite-plugin-handlebars": "^1.6.0",
		"vite-plugin-html": "^3.2.1",
		"vite-plugin-image-optimizer": "^1.1.7",
		"vite-plugin-static-copy": "^1.0.0",
		"vite-svg-sprite-wrapper": "^1.3.0"
	},
	"browserslist": [
		"cover 99.5%"
	],
